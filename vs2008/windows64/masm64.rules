<?xml version="1.0" encoding="utf-8"?>
<VisualStudioToolFile
	Name="MASM64"
	Version="8.00"
	>
	<Rules>
		<CustomBuildRule
			Name="assemble"
			CommandLine="ml64 /c /Zi /Fl [inputs]"
			Outputs="$(InputName).obj"
			FileExtensions="*.asm"
			>
			<Properties>
				<BooleanProperty
					Name="assemble"
				/>
			</Properties>
		</CustomBuildRule>
	</Rules>
</VisualStudioToolFile>
